package stdx.net.http

import std.collection.HashMap

public struct HttpStatusCode {

    /**
     * HTTP Status Code
     * RFC 9110 says: The status code of a response is a three-digit integer code that describes the result of the request and the semantics of the response,
     * including whether the request was successful and what content is enclosed (if any). All valid status codes are within the range of 100 to 599, inclusive.
     */

    /*Informational 1XX: The request was received, continuing process*/
    public static const STATUS_CONTINUE: UInt16 = 100 /* RFC 9110, 15.2.1 */
    public static const STATUS_SWITCHING_PROTOCOLS: UInt16 = 101 /* RFC 9110, 15.2.2 */
    public static const STATUS_PROCESSING: UInt16 = 102 /* RFC 2518, 10.1 */
    public static const STATUS_EARLY_HINTS: UInt16 = 103 /* RFC 8297 */

    /*Successful 2xx: The request was successfully received, understood, and accepted*/
    public static const STATUS_OK: UInt16 = 200 /* RFC 9110, 15.3.1 */
    public static const STATUS_CREATED: UInt16 = 201 /* RFC 9110, 15.3.2 */
    public static const STATUS_ACCEPTED: UInt16 = 202 /* RFC 9110, 15.3.3 */
    public static const STATUS_NON_AUTHORITATIVE_INFO: UInt16 = 203 /* RFC 9110, 15.3.4 */
    public static const STATUS_NO_CONTENT: UInt16 = 204 /* RFC 9110, 15.3.5 */
    public static const STATUS_RESET_CONTENT: UInt16 = 205 /* RFC 9110, 15.3.6 */
    public static const STATUS_PARTIAL_CONTENT: UInt16 = 206 /* RFC 9110, 15.3.7 */
    public static const STATUS_MULTI_STATUS: UInt16 = 207 /* RFC 4918, 11.1 */
    public static const STATUS_ALREADY_REPORTED: UInt16 = 208 /* RFC 5842, 7.1 */
    public static const STATUS_IM_USED: UInt16 = 226 /* RFC 3229, 10.4.1 */

    /*Redirection 3xx: Further action needs to be taken in order to complete the request*/
    public static const STATUS_MULTIPLE_CHOICES: UInt16 = 300 /* RFC 9110, 15.4.1 */
    public static const STATUS_MOVED_PERMANENTLY: UInt16 = 301 /* RFC 9110, 15.4.2 */
    public static const STATUS_FOUND: UInt16 = 302 /* RFC 9110, 15.4.3 */
    public static const STATUS_SEE_OTHER: UInt16 = 303 /* RFC 9110, 15.4.4 */
    public static const STATUS_NOT_MODIFIED: UInt16 = 304 /* RFC 9110, 15.4.5 */
    public static const STATUS_USE_PROXY: UInt16 = 305 /* RFC 9110, 15.4.6 */
    public static const STATUS_TEMPORARY_REDIRECT: UInt16 = 307 /* RFC 9110, 15.4.8 */
    public static const STATUS_PERMANENT_REDIRECT: UInt16 = 308 /* RFC 9110, 15.4.9 */

    /*Client Error 4xx: The request contains bad syntax or cannot be fulfilled*/
    public static const STATUS_BAD_REQUEST: UInt16 = 400 /* RFC 9110, 15.5.1 */
    public static const STATUS_UNAUTHORIZED: UInt16 = 401 /* RFC 9110, 15.5.2 */
    public static const STATUS_PAYMENT_REQUIRED: UInt16 = 402 /* RFC 9110, 15.5.3 */
    public static const STATUS_FORBIDDEN: UInt16 = 403 /* RFC 9110, 15.5.4 */
    public static const STATUS_NOT_FOUND: UInt16 = 404 /* RFC 9110, 15.5.5 */
    public static const STATUS_METHOD_NOT_ALLOWED: UInt16 = 405 /* RFC 9110, 15.5.6 */
    public static const STATUS_NOT_ACCEPTABLE: UInt16 = 406 /* RFC 9110, 15.5.7 */
    public static const STATUS_PROXY_AUTH_REQUIRED: UInt16 = 407 /* RFC 9110, 15.5.8 */
    public static const STATUS_REQUEST_TIMEOUT: UInt16 = 408 /* RFC 9110, 15.5.9 */
    public static const STATUS_CONFLICT: UInt16 = 409 /* RFC 9110, 15.5.10 */
    public static const STATUS_GONE: UInt16 = 410 /* RFC 9110, 15.5.11 */
    public static const STATUS_LENGTH_REQUIRED: UInt16 = 411 /* RFC 9110, 15.5.12 */
    public static const STATUS_PRECONDITION_FAILED: UInt16 = 412 /* RFC 9110, 15.5.13 */
    public static const STATUS_REQUEST_CONTENT_TOO_LARGE: UInt16 = 413 /* RFC 9110, 15.5.14 */
    public static const STATUS_REQUEST_URI_TOO_LONG: UInt16 = 414 /* RFC 9110, 15.5.15 */
    public static const STATUS_UNSUPPORTED_MEDIA_TYPE: UInt16 = 415 /* RFC 9110, 15.5.16 */
    public static const STATUS_REQUESTED_RANGE_NOT_SATISFIABLE: UInt16 = 416 /* RFC 9110, 15.5.17 */
    public static const STATUS_EXPECTATION_FAILED: UInt16 = 417 /* RFC 9110, 15.5.18 */
    public static const STATUS_TEAPOT: UInt16 = 418 /* RFC 7168, 2.3.3 */
    public static const STATUS_MISDIRECTED_REQUEST: UInt16 = 421 /* RFC 9110, 15.5.20 */
    public static const STATUS_UNPROCESSABLE_ENTITY: UInt16 = 422 /* RFC 9110, 15.5.21 */
    public static const STATUS_LOCKED: UInt16 = 423 /* RFC 4918, 11.3 */
    public static const STATUS_FAILED_DEPENDENCY: UInt16 = 424 /* RFC 4918, 11.4 */
    public static const STATUS_TOO_EARLY: UInt16 = 425 /* RFC 8470, 5.2. */
    public static const STATUS_UPGRADE_REQUIRED: UInt16 = 426 /* RFC 9110, 15.5.22 */
    public static const STATUS_PRECONDITION_REQUIRED: UInt16 = 428 /* RFC 6585, 3 */
    public static const STATUS_TOO_MANY_REQUESTS: UInt16 = 429 /* RFC 6585, 4 */
    public static const STATUS_REQUEST_HEADER_FIELDS_TOO_LARGE: UInt16 = 431 /* RFC 6585, 5 */
    public static const STATUS_UNAVAILABLE_FOR_LEGAL_REASONS: UInt16 = 451 /* RFC 7725, 3 */

    /*Server Error 5xx: The server failed to fulfill an apparently valid request*/
    public static const STATUS_INTERNAL_SERVER_ERROR: UInt16 = 500 /* RFC 9110, 15.6.1 */
    public static const STATUS_NOT_IMPLEMENTED: UInt16 = 501 /* RFC 9110, 15.6.2 */
    public static const STATUS_BAD_GATEWAY: UInt16 = 502 /* RFC 9110, 15.6.3 */
    public static const STATUS_SERVICE_UNAVAILABLE: UInt16 = 503 /* RFC 9110, 15.6.4 */
    public static const STATUS_GATEWAY_TIMEOUT: UInt16 = 504 /* RFC 9110, 15.6.5 */
    public static const STATUS_HTTP_VERSION_NOT_SUPPORTED: UInt16 = 505 /* RFC 9110, 15.6.6 */
    public static const STATUS_VARIANT_ALSO_NEGOTIATES: UInt16 = 506 /* RFC 2295, 8.1 */
    public static const STATUS_INSUFFICIENT_STORAGE: UInt16 = 507 /* RFC 4918, 11.5 */
    public static const STATUS_LOOP_DETECTED: UInt16 = 508 /* RFC 5842, 7.2 */
    public static const STATUS_NOT_EXTENDED: UInt16 = 510 /* RFC 2774, 7 */
    public static const STATUS_NETWORK_AUTHENTICATION_REQUIRED: UInt16 = 511 /* RFC 6585, 6 */

}

